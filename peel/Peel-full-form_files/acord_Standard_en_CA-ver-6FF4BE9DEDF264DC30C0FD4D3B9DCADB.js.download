function createMessagesPanelWithMessage(messages, id) {
var messagesLength = messages.length;
if (!messagesLength) {
return '';
}
id = (typeof id != 'undefined') && id.length ? id : 'id12345abc';
var messagePanelTemplate =
'<div id="' + id + '" class="messageSection-easy-section easy-section maximized" style="">' +
'<div id="' + id + '_heading" class="messageSection-easy-section-heading easy-section-heading maximized">' +
'<div id="' + id + '_topleft" class="messageSection-easy-section-heading-topleft easy-section-heading-topleft"/>' +
'<div id="' + id + '_minmax" onclick="easySectionToggleMinMax(this);" data-toggle="tooltip" ' +
'title="Collapse Section" titleexpand="Expand Section" titlecollapse="Collapse Section" ' +
'class="messageSection-easy-section-minmax easy-section-minmax maximized"/>' +
'<div id="' + id + '_icon" style="float: left; margin-top: 4px;"/>' +
'<div id="' + id + '_label" class="messageSection-easy-section-heading-label easy-section-heading-label">Messages</div>' +
'<div id="' + id + '_topright" class="messageSection-easy-section-heading-topright easy-section-heading-topright"/>' +
'<div class="table-cell-tool-bar easy-toolbar"/>' +
'</div>' +
'<div id="' + id + '_content" class="messageSection-easy-section-content easy-section-content collapsible" style="clear: both; display: block;">' +
'<ul>' +
'<div>' +
'<li>' +
'<div class="messageSection-easy-message-section-message-category-title easy-message-section-message-category-title">General</div>' +
'<ul>';
for (var i = 0; i < messagesLength; i++) {
var messageText = messages[i].messageText;
if (!messageText.length) {
continue;
}
var messageType = messages[i].messageType;
var messageTypeNumber;
switch(messageType) {
case 'info':
messageTypeNumber = '1';
break;
case 'warning':
messageTypeNumber = '2';
break;
case 'error':
messageTypeNumber = '3';
break;
case 'fatal':
messageTypeNumber = '4';
break;
default:
messageTypeNumber = '1';
}
messagePanelTemplate = messagePanelTemplate +
'<div>' +
'<li>' +
'<div style="float:left" class=" easy-message-section-message  easy-message-section-message-priority' + messageTypeNumber + '">' + messageText + '</div>' +
'<div style="float:right" class=" easy-message-section-level  easy-message-section-level-priority' + messageTypeNumber + '"/>' +
'<div style="clear:both"/>' +
'</li>' +
'</div>';
}
messagePanelTemplate = messagePanelTemplate +
'</ul>' +
'</li>' +
'</div>' +
'</ul>' +
'</div>';
messagePanelTemplate = messagePanelTemplate +
'<div style="clear:both; width:100%; height: 8px;" class="messageSection-easy-section-bottom easy-section-bottom">' +
'<div id="' + id + '_bottomleft" class="messageSection-easy-section-bottomleft easy-section-bottomleft"></div>' +
'<div id="' + id + '_bottomright" class="messageSection-easy-section-bottomright easy-section-bottomright"></div>' +
'</div>' +
'</div>';
return messagePanelTemplate;
}



