function createScrollUpButton() {

 $headerSubtitle = $(".header-subtitle");
if ($headerSubtitle.length !== 0) {

 var $icon = $('#back-to-top');
if ($icon.length === 0) {

 $("<style>")
.prop("type", "text/css")
.html("\
                    #back-to-top {\
                        position: fixed;\
                        bottom: 40px;\
                        right: 40px;\
                        z-index: 9999;\
                        width: 32px;\
                        height: 32px;\
                        text-align: center;\
                        line-height: 30px;\
                        background: #f5f5f5;\
                        color: #444;\
                        cursor: pointer;\
                        border: 0;\
                        border-radius: 2px;\
                        text-decoration: none;\
                        transition: opacity 0.2s ease-out;\
                        opacity: 0;\
                    }\
                    \
                    #back-to-top:hover {\
                        background: #e9ebec;\
                    }\
                    \
                    #back-to-top.show {\
                        opacity: 1;\
                    }\
                }")
.appendTo("head");

 $icon = $('<a href="#" id="back-to-top" title="Back to Top">&uarr;</a>');
$icon.appendTo($headerSubtitle);
if ($('#back-to-top').length) {
var scrollTrigger = 100, 
 backToTop = function () {
var scrollTop = $(window).scrollTop();
if (scrollTop > scrollTrigger) {
$('#back-to-top').addClass('show');
} else {
$('#back-to-top').removeClass('show');
}
};
backToTop();
$(window).on('scroll', function () {
backToTop();
});
$('#back-to-top').on('click', function (e) {
e.preventDefault();
$('html,body').animate({
scrollTop: 0
}, 700);
});
}
}
}
}


$(document).ajaxSend(function(event, xhr, settings) {
triggerElm = event.target.activeElement;
});
$(document).ajaxComplete(function(event, xhr, settings) {
if (triggerElm.title == 'Remove item') {
if ($(triggerElm.parentNode.parentNode.parentNode).hasClass('coverage-row')) {
initPeelAUTOCoverages();
initPeelHOMECoverages();
}
else if ($(triggerElm.parentNode.parentNode.parentNode).hasClass('persDriverSection-easy-section')) {
initPeelAUTODriverPanels();
}
else if ($(triggerElm.parentNode.parentNode.parentNode).hasClass('dwell-easy-section')) {
initPeelHOMELocationPanels();
}
}
else if (triggerElm.type == 'submit' && (triggerElm.value == 'Next' || triggerElm.value == 'I Accept')) {
triggerSubmitButtonClicked(triggerElm);
}
});
var COVERAGE_LIMIT_SELECT = "select[name$='coverageLimit:controlContainer:select']";
var COVERAGE_LIMIT_INPUT = "input[name$='coverageLimit:controlContainer:textField']";
var COVERAGE_DESCRIPTIONS = [ 'Property Damage', 'Family Protection' ];

function initPeelAUTOCoverages() {

 disableCoverageLimitInputsForCoveragesWithDescriptions(COVERAGE_DESCRIPTIONS);

 var coverageTPBIRows = getCoverageRowsWithDescription(null, 'Bodily Injury');
if (coverageTPBIRows.length) {
coverageTPBIRows.forEach(function($coverageRowSection) {
var $tpbiCoverageLimitDropDown = $($coverageRowSection).find(COVERAGE_LIMIT_SELECT);
if ($tpbiCoverageLimitDropDown.length) {

 var $currentCoveragesPanel = $tpbiCoverageLimitDropDown.closest("div.coverages");
updateCoverageLimitValueForCoveragesWithDescriptions($currentCoveragesPanel, COVERAGE_DESCRIPTIONS, $tpbiCoverageLimitDropDown.val());

 $tpbiCoverageLimitDropDown.change(function () {
$currentCoveragesPanel = $(this).closest("div.coverages");
updateCoverageLimitValueForCoveragesWithDescriptions($currentCoveragesPanel, COVERAGE_DESCRIPTIONS, $(this).val());
});
}
});
}
}

function getCoverageRowsWithDescription($coveragesPanel, descriptionText) {
var listOfCoverageRows = [];

 $("label:contains(" + descriptionText + ")", $coveragesPanel).each(function (i, obj) {

 var $coverageRowSection = $(this).closest("div[class*='coverage-row']");
if ($coverageRowSection.length) {
listOfCoverageRows.push($coverageRowSection);
}
});
return listOfCoverageRows;
}

function disableCoverageLimitInputsForCoveragesWithDescriptions(descriptionsArray) {
for (i = 0; i < descriptionsArray.length; i++) {
var coverageRows = getCoverageRowsWithDescription(null, descriptionsArray[i]);
if (coverageRows.length) {
coverageRows.forEach(function($coverageRowSection) {
var $coverageLimitInput = $($coverageRowSection).find(COVERAGE_LIMIT_INPUT);
if ($coverageLimitInput.length) {

 
 $coverageLimitInput.prop('readonly', true);
}
});
}
}
}

function updateCoverageLimitValueForCoveragesWithDescriptions($coveragesPanel, descriptionsArray, newValue) {
for (i = 0; i < descriptionsArray.length; i++) {
var coverageRows = getCoverageRowsWithDescription($coveragesPanel, descriptionsArray[i]);
if (coverageRows.length) {
coverageRows.forEach(function($coverageRowSection) {
var $coverageLimitInput = $($coverageRowSection).find(COVERAGE_LIMIT_INPUT);
if ($coverageLimitInput.length) {

 $coverageLimitInput.val(newValue);
$coverageLimitInput.attr('value', newValue);
}
});
}
}
}

var MANDATORY_COVERAGE_DESCRIPTIONS = [ 'Legal Liability/Homeowners/Tenant/Condominium', 'Personal Property/Homeowners Forms', 'Dwelling - Principal Residence' ];

function initPeelHOMECoverages() {

 
 deleteRemoveCoverageOptionForCoveragesWithDescriptions(MANDATORY_COVERAGE_DESCRIPTIONS);

 deleteRemoveCoverageOptionForWatercraftCoverages();
}

function deleteRemoveCoverageOptionForCoveragesWithDescriptions(descriptionsArray) {
for (i = 0; i < descriptionsArray.length; i++) {
var coverageRows = getCoverageRowsWithDescription(null, descriptionsArray[i]);
if (coverageRows.length) {
coverageRows.forEach(function($coverageRowSection) {
var $coverageRemoveLink = $($coverageRowSection).find('.easy-list-panel-remove-coverage');
if ($coverageRemoveLink.length) {

 $coverageRemoveLink.remove();
}
});
}
}
}

function deleteRemoveCoverageOptionForWatercraftCoverages() {
$("div.watercraftCoverages").find("div.coverage-row").each(
function(i, obj) {
var $coverageRemoveLink = $(this).find('.easy-list-panel-remove-coverage');
if ($coverageRemoveLink.length) {

 $coverageRemoveLink.remove();
}
});
}

function deleteRemoveDriverLicenseOption() {

 $("div.persDriverSection-easy-section").find("div.row").each(
function(i, obj) {
var $driverLicenseRemoveLink = $(this).find('.easy-list-panel-remove-row');
if ($driverLicenseRemoveLink.length) {

 $driverLicenseRemoveLink.remove();
}
});
}

function triggerSubmitButtonClicked($submitButton) {

 var $submittedForm = $($submitButton).closest("form");
displayMessagePanelIfValidationErrors($submittedForm);
}

function displayMessagePanelIfValidationErrors($submittedForm) {

 var $componentsWithErrors = $($submittedForm).find("[err]");
if ($componentsWithErrors.length) {

 var $messageSection = $(".messageSection-easy-message-section");
if ($messageSection.length) {

 var messages = [
{messageText: "", messageType: "info"},
{messageText: "", messageType: "warning"},
{messageText: "Mandatory information is missing. Please complete the highlighted sections below.", messageType: "error"},
{messageText: "", messageType: "fatal"}
];

 var id;
$($messageSection).children('div').each(function () {
